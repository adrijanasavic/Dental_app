<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.stomatoloska_ordinacija">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_foreground"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".splashScreen.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.LoginActivity" />
        <activity android:name=".activities.ZakazivanjeActivity"/>
        <activity android:name=".activities.PodaciZakazivanjaActivity"/>
        <activity android:name=".activities.ZakazanPregledActivity"/>
        <activity android:name=".activities.PodaciZakazanogPregleda"/>
        <activity android:name=".activities.DodavanjeKartonaActivity" />
        <activity android:name=".activities.DetaljiKartonaActivity" />
        <activity android:name=".activities.IzmenaKartonaActivity" />
        <activity android:name=".activities.ListaKartonaActivity" />

    </application>

</manifest>